subdir = src/backend/access/common
top_builddir = ../../../..
include $(top_builddir)/src/Makefile.global

OBJS = heaptuple.o indextuple.o printtup.o reloptions.o scankey.o tupdesc.o  

all: SUBSYS.o

SUBSYS.o: $(OBJS)
